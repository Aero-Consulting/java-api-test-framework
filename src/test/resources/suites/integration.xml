<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Integration tests" parallel="methods">
<test name="Интеграционные тесты" preserve-order="true" thread-count="5">
    <method-selectors>
        <method-selector>
            <selector-class name="helpers.listeners.CustomMethodSelector"/>
        </method-selector>
    </method-selectors>
    <packages>
        <package name="tests.integration"/>
    </packages>
</test>
</suite>
